{
  "name": "vanilla-parcel-boilerplate",
  "version": "1.0.0",
  "description": "Simple workflow starter for building vanilla JS apps",
  "main": "index.js",
  "scripts": {
    "dev": "npm run clean && parcel public/index.html --dist-dir development -p 3000",
    "build": "rimraf ./dist && parcel build public/*.html --dist-dir dist --public-url ./",
    "clean": "rimraf ./development && rimraf ./.parcel-cache"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ahmedelraei/django-vanilla-js-boilerplate.git"
  },
  "keywords": [
    "parcel"
  ],
  "author": "Ahmed Hatem",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ahmedelraei/django-vanilla-js-boilerplate/issues"
  },
  "homepage": "https://github.com/ahmedelraei/django-vanilla-js-boilerplate#readme",
  "devDependencies": {
    "@babel/core": "^7.17.4",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/runtime-corejs2": "^7.17.2",
    "parcel": "^2.3.1"
  },
  "dependencies": {
    "axios": "^0.26.0"
  },
  "alias": {
    "process": false
  }
}
